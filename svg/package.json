{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "search-svg-assets",
  "title": "Search SVG Assets",
  "description": "Search and copy SVG code from your team graphics library",
  "icon": "command-icon.png",
  "author": "vadim-ux",
  "categories": [
    "Design Tools",
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "search-svg",
      "title": "Search SVG Assets",
      "subtitle": "Team Graphics Library",
      "description": "Search through team SVG assets and copy code instantly",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.48.9",
    "@raycast/utils": "^1.5.2"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^8.19.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}